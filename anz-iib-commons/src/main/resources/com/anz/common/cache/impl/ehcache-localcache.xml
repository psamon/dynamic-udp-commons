<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">
	<!-- see http://www.ehcache.org/ehcache.xml -->
  	<cache name="SampleKey" maxEntriesLocalHeap="10"
	    eternal="false"
	    timeToIdleSeconds="3"
	    timeToLiveSeconds="5"
	    overflowToDisk="false">
  		<persistence strategy="none"></persistence>
  	</cache>
  	<cache name="StaticCache" maxEntriesLocalHeap="10"
	    eternal="false"
	    timeToIdleSeconds="5000"
	    timeToLiveSeconds="5000"
	    overflowToDisk="false">
  		<persistence strategy="none"></persistence>
  	</cache>
  	<cache name="MessageHeaderCache" maxEntriesLocalHeap="10"
	    eternal="false"
	    timeToIdleSeconds="130"
	    timeToLiveSeconds="130"
	    overflowToDisk="false">
  		<persistence strategy="none"></persistence>
  	</cache>
  	<cache name="TransactionIdCache" maxEntriesLocalHeap="10"
	    eternal="false"
	    timeToIdleSeconds="130"
	    timeToLiveSeconds="130"
	    overflowToDisk="false">
  		<persistence strategy="none"></persistence>
  	</cache>	
</ehcache>